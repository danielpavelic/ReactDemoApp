{
  "name": "starterkit",
  "version": "1.0.0",
  "description": "Starter kit",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3002 --subdomain daniel",
    "share": "npm-run-all --parallel open:src localtunnel",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielpavelic/starterkit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danielpavelic/starterkit/issues"
  },
  "homepage": "https://github.com/danielpavelic/starterkit#readme",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "bootstrap": "^3.3.7",
    "compression": "^1.6.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "toastr": "^2.1.2",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.8",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "cross-env": "3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "3.11.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "html-webpack-plugin": "2.24.1",
    "jsdom": "9.8.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "localtunnel": "1.8.2",
    "mocha": "3.2.0",
    "nock": "9.0.2",
    "npm-run-all": "3.1.2",
    "nsp": "2.6.2",
    "numeral": "1.5.6",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.4.1",
    "redux-immutable-state-invariant": "^1.2.4",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "1.13.3",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.2",
    "webpack-md5-hash": "0.0.5"
  }
}
